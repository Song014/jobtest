<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.project.jobtest.dao.MemberMapper">

	<insert id="MemberJoin" parameterType="member">
		insert into
			member(
				member_id
				,member_pw
				,member_phone
				,member_name
				,member_nickname
			)values(
				#{member_id}
				,#{member_pw}
				,#{member_phone}
				,#{member_name}
				,#{member_nickname}
			)
	</insert>
	
	<select id="MemberSelectOneId" parameterType="string" resultType="int">
		select
			count(*)
		from
			member
		where
			member_id = #{member_id}
	</select>
	
	<select id="MemberSelectOneNickname" parameterType="string" resultType="int">
		select
			count(*)
		from
			member
		where
			member_nickname = #{member_nickname}
	</select>
	
	<select id="MemberSelectOne" parameterType="string" resultType="member">
		select
			member_id
			,member_pw
			,member_name
			,member_phone
			,member_nickname
		from
			member
		where
			member_id = #{member_id}
	</select>

</mapper>